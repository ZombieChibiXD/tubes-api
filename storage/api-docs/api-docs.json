{
    "openapi": "3.0.0",
    "info": {
        "title": "API untuk Tubes Pemrograman",
        "version": "1.0.0"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "User login",
                "description": "Allows users to log in and obtain an authentication token",
                "operationId": "4a16cbeb901087664661f8f8b3f4c012",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/AuthLoginRequest"
                            }
                        },
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "$ref": "#/components/schemas/AuthLoginRequest"
                            }
                        },
                        "multipart/form-data": {
                            "schema": {
                                "$ref": "#/components/schemas/AuthLoginRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful login",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "properties": {
                                                "name": {
                                                    "description": "Token name",
                                                    "type": "string",
                                                    "example": "authToken"
                                                },
                                                "abilities": {
                                                    "description": "Token abilities",
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "ADMINISTRATOR"
                                                    }
                                                },
                                                "expires_at": {
                                                    "description": "Token expiration date",
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2023-06-19T15:36:53.000000Z"
                                                },
                                                "tokenable_id": {
                                                    "description": "Tokenable ID",
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "tokenable_type": {
                                                    "description": "Tokenable type",
                                                    "type": "string",
                                                    "example": "App\\\\Models\\\\User"
                                                },
                                                "updated_at": {
                                                    "description": "Updated at date",
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2023-06-19T14:36:53.000000Z"
                                                },
                                                "created_at": {
                                                    "description": "Created at date",
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2023-06-19T14:36:53.000000Z"
                                                },
                                                "id": {
                                                    "description": "Token ID",
                                                    "type": "integer",
                                                    "example": 2
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "user": {
                                            "$ref": "#/components/schemas/User"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/WrongCredentialException"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnprocessableContentException"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/auth": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Summary",
                "operationId": "cfc4b59003d22492e062884b45b3aecf",
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        },
        "/api/users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "List Users",
                "description": "Display a listing of the resource.",
                "operationId": "listUsers",
                "responses": {
                    "200": {
                        "description": "A list with users"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "UnprocessableContentException": {
                "properties": {
                    "message": {
                        "description": "Error message",
                        "type": "string",
                        "example": "Wrong username or password"
                    },
                    "errors": {
                        "description": "Validation errors",
                        "type": "object",
                        "additionalProperties": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "example": "The name field is required."
                            }
                        }
                    }
                },
                "type": "object"
            },
            "WrongCredentialException": {
                "properties": {
                    "message": {
                        "description": "Error message",
                        "type": "string",
                        "example": "Wrong username or password"
                    }
                },
                "type": "object"
            },
            "AuthLoginRequest": {
                "required": [
                    "username",
                    "password"
                ],
                "properties": {
                    "username": {
                        "description": "User's username",
                        "type": "string",
                        "example": "admin"
                    },
                    "password": {
                        "description": "User's password",
                        "type": "string",
                        "example": "password"
                    }
                },
                "type": "object"
            },
            "User": {
                "required": [
                    "id",
                    "name",
                    "email",
                    "email_verified_at",
                    "created_at",
                    "updated_at"
                ],
                "properties": {
                    "id": {
                        "description": "User ID",
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "description": "User's full name",
                        "type": "string",
                        "example": "John Doe"
                    },
                    "email": {
                        "description": "User's email address",
                        "type": "string",
                        "format": "email",
                        "example": "email@example.com"
                    },
                    "email_verified_at": {
                        "description": "Timestamp of email verification",
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-01-01 00:00:00"
                    },
                    "created_at": {
                        "description": "Timestamp of user creation",
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-01-01 00:00:00"
                    },
                    "updated_at": {
                        "description": "Timestamp of last user update",
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-01-01 00:00:00"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "description": "Masukkan token JWT",
                "name": "bearerAuth",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Auth",
            "description": "Endpoints for user authentication"
        }
    ]
}