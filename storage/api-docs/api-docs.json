                "properties": {
                    "username": {
                        "description": "User's username",
                        "type": "string",
                        "example": "admin"
                    },
                    "password": {
                        "description": "User's password",
                        "type": "string",
                        "example": "password"
                    }
                },
                "type": "object"
            },
