                        "description": "Tool material ID",
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "description": "Tool material name",
                        "type": "string",
                        "example": "John Doe"
                    },
                    "description": {
                        "description": "Description of the tool material",
                        "type": "string",
                        "example": "Karbida is a material used for tools"
                    },
                    "created_at": {
                        "description": "Timestamp of creation",
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-01-01 00:00:00"
                    },
                    "updated_at": {
                        "description": "Timestamp of last update",
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-01-01 00:00:00"
                    }
                },
                "type": "object"
            },
            "User": {
                "required": [
                    "id",
                    "name",
                    "email",
                    "email_verified_at",
                    "created_at",
                    "updated_at"
                ],
                "properties": {
                    "id": {
                        "description": "User ID",
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "description": "User's full name",
                        "type": "string",
                        "example": "John Doe"
                    },
                    "email": {
                        "description": "User's email address",
                        "type": "string",
                        "format": "email",
                        "example": "email@example.com"
                    },
                    "email_verified_at": {
                        "description": "Timestamp of email verification",
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-01-01 00:00:00"
                    },
                    "created_at": {
                        "description": "Timestamp of user creation",
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-01-01 00:00:00"
                    },
                    "updated_at": {
                        "description": "Timestamp of last user update",
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-01-01 00:00:00"
                    }
                },
                "type": "object"
            },
            "NewAccessToken": {
                "title": "New Access Token",
                "description": "Schema for a new access token response",
                "properties": {
                    "accessToken": {
                        "description": "Access token details",
                        "properties": {
                            "name": {
                                "description": "Token name",
                                "type": "string",
                                "example": "authToken"
                            },
                            "abilities": {
                                "description": "Token abilities",
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "example": "ADMINISTRATOR"
                                }
                            },
                            "expires_at": {
                                "description": "Token expiration date",
                                "type": "string",
                                "format": "date-time",
                                "example": "2023-06-19T15:36:53.000000Z"
                            },
                            "tokenable_id": {
                                "description": "Tokenable ID",
                                "type": "integer",
                                "example": 1
                            },
                            "tokenable_type": {
                                "description": "Tokenable type",
                                "type": "string",
                                "example": "App\\\\Models\\\\User"
                            },
                            "updated_at": {
                                "description": "Updated at date",
                                "type": "string",
                                "format": "date-time",
                                "example": "2023-06-19T14:36:53.000000Z"
                            },
                            "created_at": {
                                "description": "Created at date",
                                "type": "string",
                                "format": "date-time",
                                "example": "2023-06-19T14:36:53.000000Z"
                            },
                            "id": {
                                "description": "Token ID",
                                "type": "integer",
                                "example": 2
                            }
                        },
                        "type": "object"
                    },
                    "plainTextToken": {
                        "description": "Plain text token",
                        "type": "string",
                        "example": "2|QXOTSMko3kTiuZfd5YHxoBh3wwO5ywvER7ZThyWu"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "description": "Masukkan token",
                "name": "Authorization",
                "in": "header",
                "bearerFormat": "Bearer {token}",
                "scheme": "bearer"
            }
        }
    },
